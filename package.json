  "scripts": {
    "start": "webpack-dev-server --content-base test/ --open --inline --hot --compress --history-api-fallback --port 8081 --host 0.0.0.0 --config build/webpack.dev.config.js",
    "dev": "webpack --config build/webpack.dev.config.js",
    "dist:style": "gulp --gulpfile build/build-style.js",
    "dist:dev": "webpack --config build/webpack.dist.dev.config.js",
    "dist:prod": "cross-env NODE_ENV=production webpack --config build/webpack.dist.prod.config.js",
    "dist": "npm run dist:style && npm run dist:prod",
    "lint": "eslint --fix --ext .js,.vue src",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "prepublish": "npm run dist"
  },
